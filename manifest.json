{
  "name": "localpv",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "page view counter on local",
  "permissions": [
    "history",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
